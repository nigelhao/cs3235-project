{
    "main_thread": {
        "mismatch_action": "kill_process",
        "match_action": "allow",
        "filter": [
            {"syscall": "brk"},
            {"syscall": "chdir"},
            {"syscall": "clock_nanosleep"},
            {"syscall": "clone"},
            {"syscall": "close"},
            {"syscall": "execve"},
            {"syscall": "exit_group"},
            {"syscall": "faccessat"},
            {"syscall": "flock"},
            {"syscall": "fstat"},
            {"syscall": "futex"},
            {"syscall": "getpid"},
            {"syscall": "getrandom"},
            {"syscall": "ioctl"},
            {"syscall": "mmap"},
            {"syscall": "mprotect"},
            {"syscall": "munmap"},
            {"syscall": "newfstatat"},
            {"syscall": "openat"},
            {"syscall": "pipe2"},
            {"syscall": "ppoll"},
            {"syscall": "prctl"},
            {"syscall": "prlimit64"},
            {"syscall": "read"},
            {"syscall": "rt_sigaction"},
            {"syscall": "rt_sigprocmask"},
            {"syscall": "sched_getaffinity"},
            {"syscall": "sched_yield"},
            {"syscall": "set_robust_list"},
            {"syscall": "set_tid_address"},
            {"syscall": "sigaltstack"},
            {"syscall": "statx"},
            {"syscall": "tgkill"},
            {"syscall": "write"}
        ]
    }
}
