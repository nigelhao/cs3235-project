{
  "main_thread": {
      "mismatch_action": "kill_process",
      "match_action": "allow",
      "filter": [
          {"syscall": "clock_nanosleep"},
          {"syscall": "close"},
          {"syscall": "futex"},
          {"syscall": "getrandom"},
          {"syscall": "ioctl"},
          {"syscall": "mmap"},
          {"syscall": "mprotect"},
          {"syscall": "munmap"},
          {"syscall": "openat"},
          {"syscall": "read"},
          {"syscall": "rt_sigaction"},
          {"syscall": "sched_getaffinity"},
          {"syscall": "set_robust_list"},
          {"syscall": "rt_sigreturn"},
          {"syscall": "sigaltstack"},
          {"syscall": "socketpair"},
          {"syscall": "statx"},
          {"syscall": "write"},
          {"syscall": "epoll_create1"},
          {"syscall": "epoll_ctl"},
          {"syscall": "epoll_wait"},
          {"syscall": "sendto"},
          {"syscall": "recvfrom"},
          {"syscall": "clone3"},
          {"syscall": "prctl"},
          {"syscall": "rseq"},
          {"syscall": "rt_sigprocmask"},
          {"syscall": "madvise"},
          {"syscall": "mremap"}
      ]
  }
}