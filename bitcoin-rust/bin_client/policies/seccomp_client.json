{
    "main_thread": {
        "mismatch_action": "kill_process",
        "match_action": "allow",
        "filter": [    
            {"syscall": "brk"},
            {"syscall": "clock_nanosleep"},
            {"syscall": "clone"},
            {"syscall": "close"},
            {"syscall": "futex"},
            {"syscall": "getrandom"},
            {"syscall": "ioctl"},
            {"syscall": "mmap"},
            {"syscall": "mprotect"},
            {"syscall": "munmap"},
            {"syscall": "openat"},
            {"syscall": "read"},
            {"syscall": "rt_sigaction"},
            {"syscall": "sched_getaffinity"},
            {"syscall": "set_robust_list"},
            {"syscall": "rt_sigreturn"},
            {"syscall": "sigaltstack"},
            {"syscall": "socketpair"},
            {"syscall": "statx"},
            {"syscall": "write"},
            {"syscall": "exit"},
            {"syscall": "madvise"},
            {"syscall": "epoll_create1"},
            {"syscall": "epoll_ctl"},
            {"syscall": "epoll_pwait"},
            {"syscall": "exit_group"},
            {"syscall": "sendto"},
            {"syscall": "recvfrom"},
            {"syscall": "mremap"},
            {"syscall": "prctl"}
        ]
    }
}