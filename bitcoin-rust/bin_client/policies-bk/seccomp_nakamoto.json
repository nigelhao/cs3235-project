{
  "main_thread": {
      "mismatch_action": "kill_process",
      "match_action": "allow",
      "filter": [
        {"syscall": "access"},
        {"syscall": "arch_prctl"},
        {"syscall": "close"},
        {"syscall": "brk"},
        {"syscall": "execve"},
        {"syscall": "fadvise64"},
        {"syscall": "getrandom"},
        {"syscall": "mmap"},
        {"syscall": "mprotect"},
        {"syscall": "munmap"},
        {"syscall": "newfstatat"},
        {"syscall": "openat"},
        {"syscall": "pread64"},
        {"syscall": "prlimit64"},
        {"syscall": "read"},
        {"syscall": "rseq"},
        {"syscall": "set_robust_list"},
        {"syscall": "set_tid_address"},
        {"syscall": "write"},
        {"syscall": "rt_sigaction"},
        {"syscall": "sigaltstack"},
        {"syscall": "poll"},
        {"syscall": "futex"},
        {"syscall": "sched_getaffinity"},
        {"syscall": "rt_sigprocmask"},
        {"syscall": "clone3"},

        {"syscall": "socket"},
        {"syscall": "setsockopt"},
        {"syscall": "bind"},
        {"syscall": "listen"},
        {"syscall": "accept4"},
        {"syscall": "connect"},
        {"syscall": "clock_nanosleep"},
        {"syscall": "fcntl"},
        {"syscall": "recvfrom"},
        {"syscall": "exit"},
        {"syscall": "sendto"},
        {"syscall": "madvise"}
      ]
  }
}

